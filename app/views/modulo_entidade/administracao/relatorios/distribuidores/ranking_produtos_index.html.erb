      <tr>
        <th>Produto</th>
        <th>Faturamento (R$)</th>
        <th>%</th>
        <th>Quantidade</th>
        <th>%</th>
      </tr>
    </thead>
    <tbody id="resultado_relatorio"></tbody>
  </table>
</div>

